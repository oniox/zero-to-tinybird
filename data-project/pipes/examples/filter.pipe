TOKEN "filter_endpoint_read_2211" READ

NODE filter_by_symbol
SQL >

    %
    SELECT timestamp, symbol, price 
    FROM event_stream
    WHERE 
    1=1
    {% if defined(company) %}
      AND LOWER(symbol) = LOWER({{ String(company,description = 'String. Three-character symbol of interest.') }})
    {% end %}
    ORDER BY timestamp DESC
    LIMIT {{ Int32(max_results,100,description="The maximum number of reports to return per response.") }}





